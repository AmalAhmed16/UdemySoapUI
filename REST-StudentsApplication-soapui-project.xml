ata-Reader" id="92c672a1-9546-4c4f-97ac-959f72aaf4c3"><con:settings/><con:config><script>import org.apache.poi.xssf.usermodel.*

//Create object from class FileInputStream to read data by giving file path
def fs = new FileInputStream("C:\\Users\\User\\Documents\\Data-Driven\\data_excel_soupui.xlsx")

//Create object from class XSSFWorkbook to read data from excel file by passing object with file path 
def wb = new XSSFWorkbook(fs)

//Which sheet in the excel file you need to work on?
def ws = wb.getSheet("Sheet1")

//How many rows inside the sheet you need to get?
def r = ws.getPhysicalNumberOfRows()

for (def i = 0; i &lt; r; i++)
{
	def row = ws.getRow(i)
	def c = row.getPhysicalNumberOfCells()

	for(def j = 0; j &lt; c; j++)
	{
		def cell = row.getCell(j)

		//Get value from excel sheet as an integer
		log.info cell.getNumericCellValue()
		
		//Get value from excel sheet as a string
		//log.info cell.getStringCellValue()
	}
}</script></con:config></con:testStep><con:testStep type="restrequest" name="Get_Info_Student_by_ID" id="51fadac4-95e0-4bc5-b9e3-148b057782c7"><con:settings/><con:config service="http://thetestingworldapi.com" resourcePath="/api/studentsDetails" methodName="StudentsDetails 1" xsi:type="con:RestRequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:restRequest name="Get_Info_Student_by_ID" id="7df1f527-ca93-418c-b82f-01b53c3fd239" mediaType="application/json"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con:settings><con:endpoint>http://thetestingworldapi.com</con:endpoint><con:request/><con:originalUri>http://thetestingworldapi.com/api/studentsDetails</con:originalUri><con:credentials><con:authType>No Authorization</con:authType></con:credentials><con:jmsConfig JMSDeliveryMode="PERSISTENT"/><con:jmsPropertyConfig/><con:parameters/></con:restRequest></con:config></con:testStep><con:properties/></con:testCase><con:properties/></con:testSuite><con:properties/><con:wssContainer/><con:oAuth2ProfileContainer/><con:oAuth1ProfileContainer/><con:sensitiveInformation/></con:soapui-project>